{
    "family": "gestdoc-express",
    "executionRoleArn": "arn:aws:iam::084824435498:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "gestdoc-express",
            "image": "084824435498.dkr.ecr.us-east-1.amazonaws.com/gestdoc-express:express",
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "512",
    "memory": "1024"
}
